{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/data/*", "/js/*", "/css/*", "/*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "mimeTypes": {
    ".geojson": "application/geo+json",
    ".json": "application/json"
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
